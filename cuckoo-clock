#!/bin/bash
# Cuckoo-clock script 0.2
# Lara Maia Â© 2012

count=10
minutes=3

function help() {
cat << EOF
 Usage:
 $0 <times that the cuckoo will sing> <minutes of snooze>
 
 Example:
 $0 10 3
 
 To disable cdrom eject pass the param '--no-eject'
EOF
}

function exitchecks() {
	killall eject
	eject /dev/cdrom -t
	echo "Have a nice day day!"
	exit 0
}

if [ "$1" != "" ]; then count=$1; fi
if [ "$2" != "" ]; then minutes=$2; fi

while [ $# -gt 0 ]; do
	case $1 in
		-h|--help) help && exit 0 ;;
		--no-eject) noeject=true ;;
	esac
	shift
done

trap exitchecks INT TERM EXIT

cuckoo=$'524946464028000057415645666d74201000000001000100112b0000112b
